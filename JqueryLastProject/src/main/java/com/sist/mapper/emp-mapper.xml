<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.sist.mapper.emp-mapper"	>
 	<resultMap type="com.sist.vo.EmpVO" id="empMap">
 		<result property="dvo.dname" column="dname"/> 
 		<result property="dvo.loc" column="loc"/>
 		<result property="ename" column="ename"/>
 	</resultMap>
 	<select id="empListData" resultMap="empMap">
 		SELECT empno, ename, job, TO_CHAR(hiredate, 'YYYY-MM-DD') as dbday, sal, emp.deptno, dname, loc
 		FROM emp,dept
 		WHERE emp.deptno=dept.deptno
 	</select>
 </mapper>